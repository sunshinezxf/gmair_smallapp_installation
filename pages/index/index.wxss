.swiper-tab{
    width: 100%;
    border-bottom: 2rpx solid #ccc;
    text-align: center;
    height: 88rpx;
    line-height: 88rpx;
    display: flex;
    flex-flow: row;
    justify-content: space-between;
}
.swiper-tab-item{
    color:#434343;
    text-align:center;
    margin-top:5px;
}
.picker{
    padding: 5px;
    border-bottom:1px solid #ddd;
    border-top:1px solid #ddd;
}
.ptext{
  text-align:center;
  margin-bottom: 5px;
}

.scanimg{
  width:150px;
  height:150px;
  margin-bottom: 10px;
}

.icon{
  width:30px;
  height:30px;
}

.banner {  
    height: 550rpx;  
}  
 
.scrollers{
    width: 100%;
    height: 100%;
    overflow-x: hidden;
}

.active{
    color:#F65959;
    border-bottom: 4rpx solid #F65959;
}
swiper{
  text-align: center;
}

/* pages/feedback/feedback.wxss */
.radio-group {
    border-bottom: 1px solid #ddd;
    text-align: left;
}
.radio {
    display: block;
    border-top: 1px solid #ddd;
    padding: 1px;
}

button {margin: 5px;}